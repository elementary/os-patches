#, python-format
msgid "%(REMOVE)d to remove"
msgid_plural "%(REMOVE)d to remove"
msgstr[0] "%(REMOVE)d до вилучення"
msgstr[1] "%(REMOVE)d до вилучення"
msgstr[2] "%(REMOVE)d до вилучення"

